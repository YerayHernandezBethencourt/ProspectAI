    "import gradio as gr\n",
    "\n",
    "def greet(prospecto, pregunta):\n",
    "    return \"Hello, \" + prospecto + \"!\" * int(pregunta)\n",
    "\n",
    "demo = gr.Interface(\n",
    "    fn=greet,\n",
    "    inputs=[\"image\"],\n",
    "    outputs=[\"text\"],\n",
    ")\n",
    "\n",
    "demo.launch()"
   ]
